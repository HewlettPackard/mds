body(class, type, managed, boxed, prim, field, types) ::= <<
package $package(type)$;

import java.util.Objects;
import java.util.function.Consumer;

@FunctionalInterface
public interface $class$ extends Consumer<$boxed$> {
  default void accept($boxed$ val) {
    accept(val.$type.shortLower$Value());
  }
  void accept($prim$ val);

  default $class$ andThen($class$ after) {
      Objects.requireNonNull(after);
      return ($prim$ t) -> { accept(t); after.accept(t); };
  }
  default $class$ andThen(Consumer<? super $boxed$> after) {
      Objects.requireNonNull(after);
      return ($prim$ t) -> { accept(t); after.accept(t); };
  }
}
>>