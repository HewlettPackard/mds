body(class, type, jni, kind, coreType, types) ::= <<
#include <jni.h>
#include "mds-debug.h"
#include "mds_core_api.h"                           // MDS Core API
#include "mds_jni.h"
#include "array_type.h"

using namespace mds;
using namespace mds::api;
using namespace mds::jni;
using namespace mds::jni::array_type;
extern "C" {


JNIEXPORT 
jlong
JNICALL 
Java_com_hpl_mds_impl_$class$_lookupHandle
  (JNIEnv *jEnv, jobject, 
   jlong nsHIndex,
   jlong nameHIndex)
{
  return exception_handler_wr(jEnv, lookup_handle<$kind$>, nsHIndex, nameHIndex);
}


JNIEXPORT 
jboolean
JNICALL 
Java_com_hpl_mds_impl_$class$_bindHandle
  (JNIEnv *jEnv, jobject, 
   jlong nsHIndex,
   jlong nameHIndex,
   jlong valHandle)
{
  return exception_handler_wr(jEnv, bind_handle<$kind$>, nsHIndex, nameHIndex, valHandle);
}

JNIEXPORT 
jboolean
JNICALL 
Java_com_hpl_mds_impl_$class$_isSameAs
  (JNIEnv *jEnv, jobject, 
   jlong aHIndex,
   jlong bHIndex)
{
  return exception_handler_wr(jEnv, is_same_as<$kind$>, aHIndex, bHIndex);
}

JNIEXPORT 
jlong
JNICALL 
Java_com_hpl_mds_impl_$class$_createArrayHandle
  (JNIEnv *jEnv, jobject, 
   jlong size)
{
  ensure_thread_initialized(jEnv);
  return exception_handler_wr(jEnv, create_array<$kind$>, size);
}

}


>>
