package $package(type)$;

import java.util.function.BiFunction;
import java.util.function.Function;

@FunctionalInterface
public interface $class$<K> extends BiFunction<$boxed$, K, $boxed$> {

  $prim$ apply($prim$ oldVal, K key);
  
  @Override
  default $boxed$ apply($boxed$ v, K k) {
    return apply(v.$type.shortLower$Value(), k);
  }
  
  public static <K> $class$<K> ignoringKey(To$short$Function<? super $boxed$> fn) {
    return (v, k) -> fn.apply(v);
  }
  
  public static <K> $class$<K> ignoringKey(Function<? super $boxed$, ? extends $boxed$> fn) {
    return (v, k) -> fn.apply(v);
  }
  
}
